<Project>
  <Import Project="dependencies.props" />
  <Import Project="version.props" />

  <PropertyGroup>
    <Description>Simple and easy to use the log class library package</Description>
    <Product>EsWork Logging Library</Product>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com//EsWork/Es.Logging</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)Es.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <PublicSign Condition="'$(OS)' != 'Windows_NT'">true</PublicSign>

    <Authors>v.la@live.cn</Authors>
    <PackageTags>logging;log;logger</PackageTags>
    <PackageIconUrl>http://s.gravatar.com/avatar/28dfa5190889f7190e74b683fc30b827?s=80</PackageIconUrl>
    <PackageProjectUrl>https://github.com/vla/Es.Extensions.Logging.NLog</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/vla/Es.Extensions.Logging.NLog/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

</Project>
