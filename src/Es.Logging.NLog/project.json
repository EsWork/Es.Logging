{
  "name": "Es.Logging.NLog",
  "version": "1.1.0",
  "title": "Es.Logging.NLog",
  "description": "Easy Logging",
  "authors": [ "EsWork", "v.la@live.cn" ],
  "packOptions": {
    "summary": "Easy Work logging for .NET",
    "owners": [ "v.la@live.cn" ],
    "tags": [ "logging", "log", "logger" ],
    "projectUrl": "https://github.com/EsWork/Es.Logging",
    "licenseUrl": "https://github.com/EsWork/Es.Logging/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "git://github.com//EsWork/Es.Logging"
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "warningsAsErrors": true,
    "keyFile": "../Es.snk"
  },
  "frameworks": {
    "netstandard1.3": {
      "buildOptions": { "define": [ "NETSTANDARD" ] },
      "dependencies": {
        "NLog": "5.0.0-*"
      }
    },
    "net40": {
      "buildOptions": { "define": [ "NETFULL" ] },
      "dependencies": {
        "NLog": "4.3.11"
      },
      "frameworkAssemblies": {"System.Xml": "4.0.0.0"}
    },
    "net45": {
      "buildOptions": { "define": [ "NETFULL" ] },
      "dependencies": {
        "NLog": "4.3.11"
      },
      "frameworkAssemblies": { "System.Xml": "4.0.0.0" }
    }
  },
  "dependencies": {
    "Es.Logging": "1.1.0"
  }
}
